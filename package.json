{
  "name": "dhbw_electron_project",
  "version": "1.0.0",
  "description": "\"# dhbw_electron_project\"",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"DHBW Electron Project\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ichrist97/dhbw_electron_project.git"
  },
  "author": "Ivo Christ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ichrist97/dhbw_electron_project/issues"
  },
  "homepage": "https://github.com/ichrist97/dhbw_electron_project#readme",
  "dependencies": {
    "chart.js": "^2.7.3",
    "electron": "^4.0.4",
    "materialize-css": "^1.0.0-rc.2",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "electron-packager": "^13.0.1"
  }
}
