{
  "name": "dhbw_electron_project",
  "version": "1.0.0",
  "description": "\"# dhbw_electron_project\"",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon48.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Utility-Manager --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Utility Manager\"",
    "package-linux": "electron-packager . Utility-Manager --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon48.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createWinInstaller.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ichrist97/dhbw_electron_project.git"
  },
  "author": "Ivo Christ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ichrist97/dhbw_electron_project/issues"
  },
  "homepage": "https://github.com/ichrist97/dhbw_electron_project#readme",
  "dependencies": {
    "csv-writer": "^1.2.0",
    "jquery": "^3.3.1",
    "materialize-css": "^1.0.0-rc.2",
    "mysql": "^2.16.0",
    "zingchart": "^2.8.5"
  },
  "devDependencies": {
    "electron": "^4.0.7",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^3.0.4"
  },
  "build": {
    "appId": "com.ivochrist.utilitymanager",
    "productName": "Utiltiy Manager",
    "copyright": "Copyright Â© 2019 Ivo Christ"
  }
}